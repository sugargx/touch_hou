/* pages/video_list/video_list.wxss */

Page {
  background-color: white;
  padding-top: 30rpx;
  font-family: "微软雅黑";
  width: 100%;
}

.list {
  background-color: #f0f0f0;
}

.content_info {
  font-size: 30rpx;
  color: #909090;
  float: left;
  padding-top: 50rpx;
}

.finish_info {
  font-size: 35rpx;
}

.most {
  margin-bottom: 20rpx;
}


.finishVideo {
  /* margin-bottom: 20rpx; */
  width: 100%;
  height: 300rpx;
  overflow: hidden;
}
.finishVideo .bofang{
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  top: 30%;
  left: 40%;
}
.finishVideo .content {
  position: absolute;
  bottom: 0;
  left: 0;
  background-color: #000;
  opacity: 0.8;
  color: white;
  width: 100%;
  text-align: center;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size:16px;
  line-height: 1.6;
}
